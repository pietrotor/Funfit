    "aws-sdk": "^2.1402.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.7.0",
    "graphql-import-node": "^0.0.5",
    "graphql-middleware": "^6.1.34",
    "graphql-scalars": "^1.22.2",
    "graphql-tools": "^9.0.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
